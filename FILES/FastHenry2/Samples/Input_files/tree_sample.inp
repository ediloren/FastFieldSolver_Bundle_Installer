*  A fasthenry input file for a nonuniform ground plane

.units m
.default sigma=5.8e7

g1 y1=10 z1=10 x1=10 y2=11 z2=10 x2=10 y3=11 z3=11 x3=10
+ relx = 10
+ file=tree_sample.hier 
+ thick=0.1 
+ nin (,10.1,10.1)
+ nout (,10.9,10.5)
+ n3 (,10.56,10.31)
+ n4 (,10.5,11)

.external nin nout
.external nin n4
.external n3 nout

.freq fmin=1 fmax=1e9 ndec=0.1

.end
